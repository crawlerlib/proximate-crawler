{
    "name": "proximate/crawler",
    "description": "Proximate/Crawler crawls a website or part of a website via a recording HTTP proxy",
    "require": {
        "spatie/crawler": "1.3.*",
        "proximate/requester": "dev-master",
        "proximate/storage": "dev-master",
        "proximate/core": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Proximate": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/halfer/proximate-requester.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/halfer/proximate-storage.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/halfer/proximate-core.git"
        }
    ]
}
